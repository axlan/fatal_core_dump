digraph Airlock {
    ratio = 1;   // or ratio="fill"
    rankdir=LR;
    node [shape=ellipse, fontsize=12];

    EDO [label="Exterior Door Open"];
    EDC [label="Exterior Door Closed"];
    Pplus [label="Pressurizing"];
    IDC [label="Interior Door Closed"];
    IDO [label="Interior Door Open"];
    Pminus [label="Depressurizing"];

    EDO -> EDC [label="Close Exterior Door"];
    EDC -> Pplus [label="Begin Pressurizing"];
    Pplus -> IDC [label="Pressure OK"];

    IDC -> IDO [label="Open Interior Door"];
    IDO -> IDC [label="Close Interior Door"];

    IDC -> Pminus [label="Begin Depressurizing"];
    Pminus -> EDC [label="Pressure Equalized"];
    EDC -> EDO [label="Open Exterior Door"];
}
